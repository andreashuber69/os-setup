Plugin {
  type=thermal
  Config {
    NormalColor=#00ff00
    Warning1Color=#fff000
    Warning2Color=#ff0000
    AutomaticLevels=1
    Warning1Temp=140
    Warning2Temp=145
    AutomaticSensor=1
  }
}
Plugin {
  type=monitors
  Config {
    DisplayCPU=1
    DisplayRAM=1
    CPUColor=#0000FF
    RAMColor=#FF0000
  }
}
